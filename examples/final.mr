fn main() {
	var i = 0
	var inc = 0.5
	var should_loop = 1

	loop(!should_loop) {
		should_loop = match (i) {
			i<5 to { print("less than 5") }

			i>5 to {
				print("more than 5")
			}

			10 to {
				print("breaking")
				yield 0
			}

			_ to {
				print("default")
			}
		}
	}

	print("done")
}